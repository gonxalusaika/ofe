<h1>Listing cuestionarios</h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cuestionarios.each do |cuestionario| %>
      <tr>
        <td><%= cuestionario.nombre %></td>
        <td><%= button_to 'Show', cuestionario, method: :get, class: "btn btn-default" %></td>
        <td><%= button_to 'Edit', edit_cuestionario_path(cuestionario), method: :get, class: "btn btn-default" %></td>
        <td><%= button_to 'Destroy', cuestionario, method: :delete, data: { confirm: 'Are you sure?' }, class: "btn btn-default" %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cuestionario', new_cuestionario_path %>
